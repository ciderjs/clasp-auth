{
  "name": "@ciderjs/clasp-auth",
  "version": "0.1.2",
  "description": "Manage clasp credential data to use GitHub Actions secrets",
  "main": "dist/cli/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.cjs"
    }
  },
  "bin": {
    "clasp-auth": "bin/clasp-auth.js"
  },
  "scripts": {
    "build:cli": "unbuild --config build.config.cli.ts",
    "build:action": "unbuild --config build.config.action.ts",
    "check": "biome check",
    "test": "vitest run --coverage"
  },
  "keywords": [
    "google-apps-script",
    "@google/clasp",
    "ci",
    "cd",
    "github-actions",
    "github-actions-secrets"
  ],
  "author": "ciderjs/luth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ciderjs/clasp-auth.git"
  },
  "bugs": {
    "url": "https://github.com/ciderjs/clasp-auth/issues"
  },
  "packageManager": "pnpm@10.18.3",
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.3",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "4.0.6",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.6"
  }
}
